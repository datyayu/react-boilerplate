{
  "name": "yayu-react-boilerplate",
  "version": "1.0.0",
  "description": "A cool boilerplate for new react projects.",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js",
    "test": "NODE_ENV=production ava",
    "watch-tests": "NODE_ENV=production ava --watch"
  },
  "author": "Arturo Coronel <aoitsu3@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-prefer-object-spread": "^1.1.0",
    "eslint-plugin-react": "^5.2.2",
    "express": "^4.14.0",
    "ignore-styles": "^4.0.0",
    "jsdom": "^9.2.1",
    "node-sass": "^3.8.0",
    "nodemon": "^1.9.2",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-hot-loader": "^3.0.0-beta.0",
    "require-dir": "^0.3.0",
    "sass-loader": "^3.2.1",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "ava": {
    "require": [
      "babel-register",
      "ignore-styles",
      "./test/_config"
    ],
    "babel": {
      "presets": [
        "es2015",
        "react",
        "stage-0"
      ],
      "plugins": [
        "transform-object-rest-spread"
      ]
    }
  }
}
